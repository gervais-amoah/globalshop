{
  "name": "globalshop",
  "version": "1.0.0",
  "description": "eCommencer web app built withthe MERN stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:insert": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
  "author": "Gervais Yao Amoah",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "dotenv": "^16.3.1"
  }
}
